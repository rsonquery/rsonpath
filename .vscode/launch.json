    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'rsonpath'",
            "env": {
                "RUSTFLAGS": "-C target-cpu=native"
            },
            "cargo": {
                "args": [
                    "build",
                    "--bin=rsonpath",
                ],
                "filter": {
                    "name": "rsonpath",
                    "kind": "bin"
                }
            },
            "args": [
                "$..datavalue..value..id",
                "./crates/rsonpath-lib/tests/data/wikidata/wikidata_person.json",
                "-e",
                "main",
                "-v"
            ],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in library 'rsonpath-lib'",
            "env": {
                "RUSTFLAGS": "-C target-cpu=native"
            },
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--lib",
                    "--package=rsonpath-lib",
                ]
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
